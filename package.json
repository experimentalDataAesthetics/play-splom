{
  "name": "play-splom",
  "productName": "PlaySPLOM",
  "companyName": "experimentalDataAesthetics",
  "version": "0.1.0",
  "description": "Scatter plot sonification for exploring datasets with sound",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/experimentalDataAesthetics/play-splom.git"
  },
  "author": {
    "name": "Chris Sattinger",
    "email": "crucialfelix@gmail.com",
    "url": "http://ixdm.ch/critical-media-lab"
  },
  "main": "main.js",
  "electronVersion": "0.37.6",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint app test *.js",
    "hot-server": "node server.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress --profile --colors",
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.config.node.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron ./",
    "package": "cross-env NODE_ENV=production node package.js",
    "package-all": "npm run package -- --all",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\""
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "keywords": [
    "datavisualization",
    "sonification",
    "sound",
    "supercolider",
    "d3",
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "baconjs": "^0.7.84",
    "d3": "^3.5.16",
    "electron-debug": "^0.7.0",
    "font-awesome": "^4.6.1",
    "fs-jetpack": "^0.8.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.11.1",
    "material-ui": "^0.15.0-beta.1",
    "miso.dataset": "^0.4.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hyperscript": "^2.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-slider": "^0.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.1",
    "supercolliderjs": "^0.11.0",
    "updeep": "0.14.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "debug-menu": "^0.4.0",
    "del": "^2.2.0",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.6",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.6.0",
    "jsdom": "^8.4.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.4.5",
    "node-libs-browser": "^1.0.0",
    "react-addons-test-utils": "^15.0.1",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-filterable-log-monitor": "^0.6.4",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-logger": "^2.6.1",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
