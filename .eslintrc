{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-no-bind": 0,
    "react/prefer-stateless-function": 0,
    "react/prefer-es6-class": 1,
    "react/prop-types": 1,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "func-names": 1,
    "prefer-arrow-callback": 1,
    "arrow-body-style": 0,
    "space-before-function-paren": 0,
    "no-var": 1,
    "one-var": 1,
    "quotes": 1,
    "vars-on-top": 1,
    "one-var-declaration-per-line": 1,
    "padded-blocks": 0,
    "prefer-template": 1,
    "prefer-const": 1,
    "object-shorthand": 1,
    "object-curly-spacing": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "max-len": 1
  },
  "plugins": [
    "react"
  ]
}
