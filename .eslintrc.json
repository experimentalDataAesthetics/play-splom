{
    "extends": "eslint:recommended",
    "plugins": [
        "react"
    ],
     "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jasmine": true
    },
    "globals": {
      "jest": true,
      "describe": true,
      "expect": true,
      "it": true,
      "pit": true,
      "spyOn": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
